
import java.util.*;

class Solution {
	public String solution(int n) {
		String answer = "";
		for (int i = 0; i < n; i++) {
			if (i % 2 == 0) {
				answer = answer + "수";
			} else {
				answer = answer + "박";
			}
		}
		return answer;
	}
    public static void main(String[] args){
        Solution sol = new Solution();
        Scanner sc = new Scanner(System.in);
        int k=0;
		k = sc.nextInt();
        System.out.println(sol.solution(k));
    }
}
